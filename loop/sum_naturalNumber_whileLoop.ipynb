   ],
   "source": [
    "num = int(input(\"Enter a positive integer: \"))\n",
    "sum = 0\n",
    "while(num > 0):\n",
    "    sum += num\n",
    "    num -= 1\n",
    "print(\"The sum of natural numbers is\", sum)"
   ]
